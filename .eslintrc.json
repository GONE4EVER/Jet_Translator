{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals":{
        "webix":true,
        "scheduler":true,
        "APPNAME":true, "VERSION":true, "PRODUCTION":true
    },
    "extends": "./node_modules/eslint-xbsoftware/1__double_quotes_and_tabs/.eslintrc",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-debugger": "off",
        "indent": [
            "error",
            "tab",
            {"SwitchCase":1}
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
	"plugins":[
		"import"
	]
}
